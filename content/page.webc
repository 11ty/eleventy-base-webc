---js
function permalink(data) {
	return "/remapped/";
}

// 这里模拟从Java后端获取的数据
const json = {
  'menu 1': [
    { title: 'sub_menu1-1', link: "/a.html" },
    { title: 'sub_menu1-2', link: "/b.html" }
  ],
  'menu 2': [
    { title: 'sub_menu2-1', link: "/c.html" },
